<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="line">
            <stroke
                android:color="@android:color/white"
                android:width="2dp" />
        </shape>
    </item>

    <item>
        <rotate
            android:fromDegrees="90"
            android:toDegrees="-90"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="line">
                <stroke
                    android:color="@android:color/white"
                    android:width="2dp" />
            </shape>
        </rotate>
    </item>

</layer-list>