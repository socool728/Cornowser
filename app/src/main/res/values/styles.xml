<resources>
    <style name="Crunchy" />

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Xquid_NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">

    </style>

    <style name="CoreMTRLTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>


    <style
        name="Crunchy.ProgressView"
        parent="Material.Widget.ProgressView" />

    <style
        name="Crunchy.ProgressView.Linear"
        parent="Material.Widget.ProgressView.Linear" />

    <style
        name="Crunchy.ProgressView.Linear.Determinate"
        parent="Material.Widget.ProgressView.Linear"/>

    <style
        name="Crunchy.ProgressView.Linear.Determinate.Main"
        parent="Crunchy.ProgressView.Linear.Determinate">
        <item name="cpd_strokeColor">@color/blue_600</item>
        <item name="cpd_strokeSecondaryColor">@color/blue_500_st</item>
        <item name="android:background">@color/blue_600_t</item>
        <item name="pv_progress">0</item>
    </style>

</resources>
